{
    "manifest_version": 3,
    "name": "Native Messaging Extension",
    "version": "1.0",
    "description": "A simple Chrome extension for sample native messaging.",
    "permissions": [
        "nativeMessaging"
    ],
    "action": {
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        },
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}